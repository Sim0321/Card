{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@styles/*": ["src/styles/*"],
      "@components/*": ["src/components/*"],
      "@shared/*": ["src/components/shared/*"],
      "@contexts/*": ["src/contexts/*"],
      "@models/*": ["src/models/*"],
      "@pages/*": ["src/pages/*"]
    },
    "forceConsistentCasingInFileNames": true,
    "strict": true
  }
}
